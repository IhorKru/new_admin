{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "beberlei/DoctrineExtensions": "^1.0",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/orm": "^2.6",
        "egulias/email-validator": "~2.1",
        "gender-api/client": "^1.0",
        "neverbounce/neverbounce-php": "~4.1.0",
        "prewk/xml-string-streamer": "^0.11.0",
        "react/http": "^0.8.0",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.0",
        "symfony/config": "^4.0",
        "symfony/console": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/dom-crawler": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/security": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/web-server-bundle": "^4.0",
        "symfony/yaml": "^4.0",
        "twbs/bootstrap": "4.0.0-beta.3",
        "twig/twig": "~2.0"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C1QQNBJJR798Z70VS1JAP7V9",
            "allow-contrib": false
        },
        "incenteev-parameters": {
            "file": ".env",
            "env-map": {
                "DATABASE_USER": "RDS_USERNAME",
                "DATABASE_PWD": "RDS_PASSWORD",
                "DATABASE_NAME": "RDS_DB_NAME",
                "DATABASE_HOST": "RDS_HOSTNAME",
                "DATABASE_PORT": "RDS_PORT"
            }
        }
    }
}
